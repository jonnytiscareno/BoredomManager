
<h1> Listing Activities </h1>	

<table>

<tr>
	<th> Activity </th>
	<th> Length (Minutes) </th>
	<th> Description </th>

</tr>


<tbody>
	<% @all_activities.each do |t| %>
		<tr>
			<td> <%= t.name %> </td>
			<td> <%= t.length %> </td>
			<td> <%= t.description %> </td>
		</tr>
	  <% end %>
</tbody>




 